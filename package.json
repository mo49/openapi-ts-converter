  "dependencies": {
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mo49/openapi-ts-converter.git"
  },
  "bugs": {
    "url": "https://github.com/mo49/openapi-ts-converter/issues"
  },
  "homepage": "https://github.com/mo49/openapi-ts-converter#readme",
