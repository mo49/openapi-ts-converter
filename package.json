{
  "name": "openapi-ts-converter",
  "version": "1.1.0",
  "description": "CLI tool to convert OpenAPI schema definitions to TypeScript type definitions, including request parameters",
  "main": "dist/index.js",
  "bin": {
    "openapi-ts-converter": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
